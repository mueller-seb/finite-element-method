function [ eta ] = etaAPost(u, bvp)
%ETA Eta of a posteriori error estimation
%   Detailed explanation goes here

    function valueK = etaKsqr(domain)
        h_K = domain.diameter;
        gradU_K = u.shapeFunction(domain).gradient;
        laplaceU_K = u.shapeFunction(domain).laplace;
        r_K = @(x, y) f(x,y) + laplaceU_K.evaluate(x,y);
        
        edgeTerm = 0;
        for E = domain.edges(1:end)
            n_E = E.normVec;
            r_E = @(x, y) dot(n_E, gradU_K.evaluate(x,y));
            h_E = E.length;
            edgeTerm = edgeTerm + h_E * gaussQuad(r_E, E.nodes, 3);
        end
        edgeTerm = edgeTerm/2;
        
        interiorTerm = h_K^2 * gaussQuad(r_K, domain, 3);
        valueK = interiorTerm + edgeTerm;       
    end

eta = 0
for K = u.ansatzFunctionSpace.Mesh.domains(1:end)
    

end

